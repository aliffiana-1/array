public class ArrayTugasPratikum1 {
    public static void main(String[] args) {
       /* int bilangan[5] = {1, 2, 3, 4, 5}; program akan error karena dalam kurung siku pada tipe data harus kosong
        System.out.println(bilangan[6]: program akan eror karena pada inputannya hanya ada 4 indeks
        */
         int bilangan [] = {1,2,3,4,5};
         for (int i = 0; i < bilangan.length; i++) {
             System.out.println(bilangan[i]);
             System.out.print(" ");
             
             
        }
       
    }
   
    
}
