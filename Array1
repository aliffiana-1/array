public class Array {

    public static void main(String[] args) {
       int nilai[]= new int [3];
       nilai [0] = 100;
       nilai [1] = 20;
       nilai [2] = 90;
       double ratarata = 0.0;
       for(int i = 0; i<nilai.length; i++)ratarata+=nilai[i];
       ratarata/=nilai.length;
        System.out.println("Nilai rata-rata = "+ ratarata);
        
   
            }
    
}
